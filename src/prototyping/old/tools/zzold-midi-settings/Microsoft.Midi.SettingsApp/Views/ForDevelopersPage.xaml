<Page
    x:Class="Microsoft.Midi.SettingsApp.Views.ForDevelopersPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:labs="using:CommunityToolkit.Labs.WinUI"
    mc:Ignorable="d">

    <ScrollViewer x:Name="ContentArea">

        <labs:SettingsExpander x:Name="LoggingSettingsExpander"
                               Description="The SettingsExpander has the same properties as a Card, and you can set SettingsCard as part of the Items collection."
                               Header="Logging"
                               IsExpanded="False">



        </labs:SettingsExpander>

    </ScrollViewer>
</Page>
